{
    "name": "ecotone/pdo-event-sourcing",
    "license": "MIT",
    "homepage": "https://docs.ecotone.tech/",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Dariusz Gafka",
            "email": "dgafka.mail@gmail.com"
        }
    ],
    "keywords": ["dbal", "ecotone"],
    "description": "Extends Ecotone with Event Sourcing using Prooph PDO Event Store",
    "autoload": {
        "psr-4": {
            "Ecotone\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ecotone\\": ["tests", "vendor/ecotone/ecotone/tests"]
        },
        "psr-0": {
            "FixtureAutoloadTest\\": "tests\\Messaging",
            "FixtureIncorrectNamespace\\": "tests\\Messaging"
        }
    },
    "require": {
        "php": ">=8.0",
        "ecotone/dbal": "^1.0",
        "prooph/pdo-event-store": "^1.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "behat/behat": "^3.3.1",
        "doctrine/dbal": "^2.1",
        "doctrine/persistence": "^1.3",
        "phpstan/phpstan": "^0.12"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
